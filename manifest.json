{
  "name": "ESM Exporter",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This extension was created to export all subscribers from esm",
  "homepage_url": "https://wonkasoft.com/esm-exporter",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "ESM Exporter",
    "default_popup": "src/page_action/page_action.html"
  },
  "permissions": [
    "notifications",
    "activeTab",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.evolutionsolutionmarketing.com/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}